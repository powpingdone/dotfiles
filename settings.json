{
  "remote.containers.dockerPath": "/var/home/powpingdone/.local/bin/podman-host",
  "emmet.includeLanguages": {
    "rust": "html",
  },
  "workbench.colorTheme": "Edge Dark",
  "editor.fontFamily": "\"Source Code Pro\",'Droid Sans Mono', 'monospace', monospace",
  "editor.inlayHints.padding": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "workbench.colorCustomizations": {
    "[Edge Dark]": {
      "editorInlayHint.background": "#424553f0",
      "editorInlayHint.foreground": "#c5cdd9",
    },
  },
  "[toml]": {
    "editor.defaultFormatter": "bodil.prettier-toml"
  },
  "window.titleBarStyle": "custom",
  "vscode-pets.petSize": "medium",
  "vscode-pets.petType": "dog",
  "vscode-pets.petColor": "yellow",
  "vscode-pets.theme": "forest",
  "C_Cpp.intelliSenseEngine": "disabled",
  "[cpp]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "custom-format.formatters": [
    {
      "language": "slint",
      "command": "/var/home/powpingdone/extern/slint/target/release/slint-fmt /dev/stdin"
    }
  ],
  "auto-build.defaultEnv.name": "rambo",
  "auto-build.build.silent": false,
  "rust-analyzer.rustfmt.overrideCommand": [
    "/cargo/bin/genemichaels"
  ],
  "git.openRepositoryInParentFolders": "never"
}
